<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>{{personal.name}} - {{personal.jobtitle}} Resume</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet">
    <link href="vendor/font-mfizz-2.4.1/font-mfizz.css" rel="stylesheet">
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="css/resume.min.css" rel="stylesheet">

</head>
<body id="page-top">
<nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
    <a class="navbar-brand js-scroll-trigger" href="#page-top">
        <span class="d-block d-lg-none">{{personal.name}}</span>
        <span class="d-none d-lg-block">
          <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="{{personal.picture}}" alt="">
        </span>
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#about">About</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#experience">Experiences</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#projects">Projects</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#skills">Skills</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#awards_lectures_education">Certificates</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#awards_lectures_education">Given Lectures</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#awards_lectures_education">Education</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#interests_references">Interests</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#interests_references">References</a>
            </li>
        </ul>
    </div>
</nav>

<div class="container-fluid p-0">

    <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
            <h1 class="mb-0">{{personal.name}}
                <span class="text-primary">{{personal.surname}}</span>
            </h1>
            <div class="subheading">
                {{personal.jobtitle}}
            </div>
            <div class="subheading mb-5">{{details.address}} · {{details.phone}} ·
                <a href="mailto:{{details.email}}">{{details.email}}</a>
            </div>
            <p class="lead mb-5">
            <blockquote class="blockquote mb-5">
                {{personal.bio}}
            </blockquote>
            </p>
            <div class="social-icons">
                <a href="https://www.linkedin.com/in/{{details.linkedin}}" target="_blank"
                   style="background-color:#0077B5 !important">
                    <i class="fab fa-linkedin"></i>
                </a>
                <a href="https://github.com/{{details.github}}" target="_blank"
                   style="background-color:#99999 !important;">
                    <i class="fab fa-github"></i>
                </a>
                <a href="skype:{{details.skypeid}}" target="_blank" style="background-color:#007bff !important;">
                    <i class="fab fa-skype"></i>
                </a>
                <a href="{{personal.pdf}}" target="_blank" style="background-color:red !important;">
                    <i class="fas fa-file-pdf"></i>
                </a>
            </div>
        </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">

        <div class="my-auto">
            <h2 class="mb-5">Experience</h2>
            {{#exp}}
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                    <div class="resume-content mr-auto">
                        <h3 class="mb-0">{{title}}</h3>

                        <div class="subheading mb-3">{{company}}</div>
                        <p>{{desc}}</p>
                        <div>
                            <ul class="fa-ul mb-0">
                                {{#details}}
                                    <li><i class="fa-li fa fa-check"></i>{{text}}</li>
                                {{/details}}
                            </ul>
                        </div>
                    </div>
                    <div class="resume-date text-md-right">
                        <span class="text-primary">{{start}} - {{end}}</span>
                    </div>
                </div>
            {{/exp}}
        </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="projects">
        <div class="my-auto">
            <h2 class="mb-5">Projects</h2>
            {{#projects}}
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                    <div class="resume-content mr-auto">
                        <h3 class="mb-0">{{title}}</h3>
                        <!-- <div class="subheading mb-3">Bachelor of Science</div> -->
                        <div>
                            <ul class="fa-ul mb-0">
                                {{#details}}
                                    <li><i class="fa-li fa fa-check"></i>{{text}}</li>
                                {{/details}}
                            </ul>
                        </div>
                    </div>
                    <div class="resume-date text-md-right">
                        <span class="text-primary">{{company}}</span>
                    </div>
                </div>
            {{/projects}}
        </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="skills">
        <div class="my-auto">
            <h2 class="mb-5">Skills</h2>

            <div class="subheading mb-3">Programming Languages &amp; Tools</div>
            <ul class="list-inline dev-icons">
                <li class="list-inline-item">
                    <i class="icon-java" style="color:blue"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-scala" style="color:red"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-ruby" style="color:red"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-python" style="color:darkblue"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-nginx" style="color:green"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-maven" style="color:blue"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-redhat" style="color:red"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-tomcat" style="color:orange"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-ubuntu" style="color:brown"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-aws" style="color:orange"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-postgres" style="color:darkblue"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-mysql" style="color:darkblue"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-oracle" style="color:red"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-spring" style="color:green"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-ruby-on-rails" style="color:red"></i>
                </li>
            </ul>

            <ul class="mb-5">
                {{#additional_skills}}
                    <li><i class="fa-li fa fa-check" style="color:darkgray"></i>{{title}}</li>
                {{/additional_skills}}
            </ul>

            <div class="subheading mb-3">Workflow</div>
            <ul class="fa-ul mb-0">
                <li>
                    <i class="fa-li fa fa-check" style="color:red"></i>
                    Accept and Analyze Problem
                </li>
                <li>
                    <i class="fa-li fa fa-check" style="color:brown"></i>
                    Find most suitable solution
                </li>
                <li>
                    <i class="fa-li fa fa-check" style="color:orange"></i>
                    Develop most efficiently
                </li>
                <li>
                    <i class="fa-li fa fa-check" style="color:green"></i>
                    Agile Development &amp; Scrum
                </li>
            </ul>
        </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="awards_lectures_education">
        <div class="my-auto">
            <h2 class="mb-3">Awards &amp; Certifications</h2>
            <ul class="fa-ul mb-0">
                {{#certificates}}
                    <li class="mt-2"><i class="fa-li fa {{icon}}"></i>
                        {{title}}</li>
                {{/certificates}}
            </ul>
            <p>
            <ul class="list-inline dev-icons">
                <li class="list-inline-item">
                    <i class="icon-oracle-alt" style="color:red;"></i>
                </li>
                <li class="list-inline-item">
                    <i class="icon-java" style="color:darkblue"></i>
                </li>
            </ul>
            </p>
        </div>
        <div class="my-auto">
            <h2 class="mb-2">Given Lectures</h2>
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <ul class="fa-ul mb-0">
                        {{#given_lectures}}
                            <li class="mt-2"><i class="fa-li fa fa-check"></i>{{title}}</li>
                        {{/given_lectures}}
                    </ul>
                </div>
            </div>
        </div>
        <div class="my-auto">
            <h2 class="mb-3">Education</h2>

            {{#edu}}
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                    <div class="resume-content mr-auto">
                        <h3 class="mb-0">{{title}}</h3>
                        <div class="subheading mb-3">{{department}}</div>
                        <p>{{desc}}</p>
                    </div>
                    <div class="resume-date text-md-right">
                        <span class="text-primary">{{start}} - {{end}}</span>
                    </div>
                </div>
            {{/edu}}
        </div>
    </section>

    <hr class="m-0">

    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="interests_references">
        <div class="my-auto">
            <h2 class="mb-5">Interests</h2>
            <ul>
                {{#interests}}
                    <li class="mb-2"><span>{{title}}</span></li>
                {{/interests}}
            </ul>
        </div>
        <div class="my-auto">
            <h2 class="mb-5">References</h2>
            <ul>
                <li><span>References will be supplied in case of request.</span></li>
            </ul>
        </div>
    </section>

    <hr class="m-0">

</div>

<!-- Bootstrap core JavaScript -->
<script src="vendor/jquery/jquery.min.js"></script>
<script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Plugin JavaScript -->
<script src="vendor/jquery-easing/jquery.easing.min.js"></script>

<!-- Custom scripts for this template -->
<script src="js/resume.min.js"></script>
</body>
</html>
